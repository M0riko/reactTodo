{
    "version": 2,
    "builds": [{ "src": "package.json", "use": "@vercel/node" }],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/api/.*",
        "dest": "api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "functions": { "api/index.js": { "runtime": "nodejs16" } }
  }
  